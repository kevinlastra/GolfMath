#include <iostream>
#include "Terrain.h"
void PrintMap(Terrain &t)
{
  for(int i = 0; i < t.longueur;i++)
  {
    for(int j = 0; j < t.longueur;j++)
    {
      if(t.getNode(Vector(i,j)).type == Type.eau)
	std::cout << "@";
      else
	std::cout << "0";
    }
    std::cout << std::endl;
  }
}
int main(int argc, char** argv)
{
  Terrain T(argv[1]);
  return 1;
}
